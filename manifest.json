{
	"manifest_version": 3,
	"name": "SampleWizard",
	"version": "1.0.0",
	"action": { 
		"default_popup": "index.html" 
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [
		"tabs",
		"tabCapture",
		"activeTab",
		"tabCapture",
		"offscreen",
		"unlimitedStorage",
		"storage",
		"downloads"
	]
  }